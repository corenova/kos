{
  "name": "kos",
  "version": "1.0.0",
  "description": "A dataflow streaming framework for creating awesome data pipelines and state machines",
  "bin": {
    "kos": "./bin/kos.js"
  },
  "main": "index.js",
  "browser": "browser.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corenova/kos.git"
  },
  "keywords": [
    "adaptive",
    "dataflow",
    "react",
    "kinetic",
    "stream",
    "pipeline",
    "pipe",
    "flow",
    "action",
    "transform"
  ],
  "author": "Peter K. Lee <peter@corenova.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/corenova/kos/issues"
  },
  "homepage": "https://github.com/corenova/kos#readme",
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean -s && mkdirp dist",
    "build:min": "browserify -t babelify -p [ minifyify --no-map ] . -o dist/kos.min.js",
    "build:full": "browserify -t babelify . -o dist/kos.js",
    "build": "npm run build:full && npm run build:min",
    "test": "mocha"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "circular-json": "^0.3.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "debug": "^2.6.0",
    "delegates": "^1.0.0",
    "treeify": "^1.0.1",
    "uuid": "^3.0.1"
  },
  "optionalDependencies": {
    "simple-websocket": "^5.0.0",
    "superagent": "^3.4.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "minifyify": "^7.3.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "should": "^11.2.0"
  }
}
